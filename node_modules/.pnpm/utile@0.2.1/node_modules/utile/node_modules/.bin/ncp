#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/marioteik/Projects/Cli/node_modules/.pnpm/ncp@0.4.2/node_modules/ncp/bin/node_modules:/home/marioteik/Projects/Cli/node_modules/.pnpm/ncp@0.4.2/node_modules/ncp/node_modules:/home/marioteik/Projects/Cli/node_modules/.pnpm/ncp@0.4.2/node_modules:/home/marioteik/Projects/Cli/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/marioteik/Projects/Cli/node_modules/.pnpm/ncp@0.4.2/node_modules/ncp/bin/node_modules:/home/marioteik/Projects/Cli/node_modules/.pnpm/ncp@0.4.2/node_modules/ncp/node_modules:/home/marioteik/Projects/Cli/node_modules/.pnpm/ncp@0.4.2/node_modules:/home/marioteik/Projects/Cli/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ncp@0.4.2/node_modules/ncp/bin/ncp" "$@"
else
  exec node  "$basedir/../../../../../ncp@0.4.2/node_modules/ncp/bin/ncp" "$@"
fi
